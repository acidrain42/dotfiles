{
    "python.formatting.blackPath": "/usr/bin/black",
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": ["-l", "120"],
    "python.jediEnabled": false,
    "python.linting.flake8Enabled": false,
    "python.linting.flake8Args": ["--ignore=E203,E501,W503"],
    "python.linting.pylintEnabled": true,
    "python.linting.pylintPath": "/usr/bin/pylint",
    "python.venvPath": "~/.virtualenvs",
    "git.enableGutters": false,
    "yaml.customTags": [ "!GetAtt", "!Join", "!Ref", "!Sub"],
    "rust-analyzer.serverPath": "/usr/bin/rust-analyzer"
}
